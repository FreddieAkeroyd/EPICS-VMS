# MCC (Motor Control Center) pvs
# these are temporary pvs, fed from SPEAR2 until communication (UDP)
# is done on IOC
#

record(ai, "$(mcc):CurrA")
{
        field(DESC, "Phase IA current")
        field(DTYP, "Soft Channel")
        field(EGU,  "A")
        field(PREC, "1")
        field(LOLO, "-999" )
        field(HOPR, "$(currDrvh)")
        field(LOPR, "0.0")
        field(LLSV, "INVALID")
}

